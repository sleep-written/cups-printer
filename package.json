{
  "name": "cups-printer",
  "description": "A package to print documents using CUPS Printer Server in Linux",
  "version": "0.2.0",
  "type": "module",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "files": [ "dist" ],
  "scripts": {
    "build": "rm -rf ./dist && tsc -p ./tsconfig.build.json",
