    "test": "clear && ava"
  },
  "author": "sleep-written",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sleep-written/cups-printer.git"
  },
  "bugs": {
    "url": "https://github.com/sleep-written/cups-printer/issues"
  },
  "homepage": "https://github.com/sleep-written/cups-printer#readme",
  "categories": [
    "Other"
  ],
  "keywords": [
    "cups",
    "printer",
    "server",
    "bash",
    "pdf"
  ],
  "devDependencies": {
    "@types/node": "^18.7.6",
    "ava": "^4.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
